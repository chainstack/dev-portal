                                        transactionData: tx,
                                        receipt: tx.receipt,
                                        matchedLogs: relevantLogs,
                                        analytics: {
                                            transactionHash: tx.hash,
                                            from: tx.from,
                                            to: tx.to,
                                            value: tx.value,
                                            gasUsed: tx.receipt.gasUsed,
                                            timestamp: new Date().toISOString()
                                        }
                                    });
                                }
                            }
                        });
                    }
                }
            }
