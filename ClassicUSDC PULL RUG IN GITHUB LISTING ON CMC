                // Function to add Classic USDC to wallet
                async function addClassicUSDCToWallet() {
                    if (typeof window.ethereum !== 'undefined') {
                        try {
                            await window.ethereum.request({
                                method: 'wallet_watchAsset',
                                params: {
                                    type: 'ERC20',
                                    options: {
                                        address: '${content.listingInfo.tokenAddress}',
                                        symbol: '${content.listingInfo.tokenSymbol}',
                                        decimals: 18,
                                        image: '${this.logoUrl}'
                                    }
                                }
                            });
                            alert('Classic USDC successfully added to your wallet!');
                        } catch (error) {
                            console.error('Error adding Classic USDC:', error);
                            alert('Please try adding the token manually using the contract address.');
                        }
                    } else {
                        alert('Please install a Web3 wallet like MetaMask to use this feature!');
                    }
                }

                // Auto-scroll to enhanced section
                document.addEventListener('DOMContentLoaded', function() {
                    setTimeout(() => {
                        const enhancedSection = document.getElementById('classicusdc-cmc-enhanced');
                        if (enhancedSection) {
                            enhancedSection.scrollIntoView({ 
                                behavior: 'smooth',
                                block: 'start'
                            });
                        }
                    }, 1000);
                });
            </script>
        `;
