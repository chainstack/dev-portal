{
  "openapi": "3.0.0",
  "info": {
    "title": "wallet/getnowblock TRON API",
    "version": "1.0.0",
    "description": "Get the current latest block from the TRON blockchain"
  },
  "servers": [
    {
      "url": "https://tron-mainnet.core.chainstack.com"
    }
  ],
  "paths": {
