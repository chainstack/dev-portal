                        },
                        "witness_signature": {
                          "type": "string"
                        }
                      }
                    },
                    "transactions": {
                      "type": "array"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "x-readme": {
    "explorer-enabled": true,
    "proxy-enabled": true
